#include <stdio.h>

int main(){
  double matrix1[3][3];
  for (int i = 0; i<3; i++){
    for (int j = 0; i<3; i++){
      printf("Matrix number 1 row %d column %d: ", i, j);
      scanf("%d",&matrix1[i][j]);
    }
  }
  double matrix2[3][3];
  for (int i = 0; i<3; i++){
    for (int j = 0; i<3; i++){
      printf("Matrix number 2 row %d column %d: ", i, j);
      scanf("%d",&matrix2[i][j]);
    }
  }
  double matrix3[3][3];
  for (int i = 0; i<3; i++){
    for (int j = 0; i<3; i++){
      matrix3[i][j] = matrix1[i][j] - matrix[i][j];
    }
  }
  for (int i = 0; i<3; i++){
    for (int j = 0; i<3; i++){
      printf("After substraction, final matrix number row %d column %d: %d", i, j, matrix3[i][j]);
    }
  }
  

}
