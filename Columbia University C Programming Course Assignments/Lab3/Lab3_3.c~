  }

int lengthof_int(int num){
  int count = 0;
  while (num != 0){
    num /= 10;
    count += 1;
  }
  return count;
}



int main(){
  
  int num, d, dp;
  printf("Enter the number: ");
  scanf("%d",&num);
  printf("Enter d: ");
  scanf("%d",&d);
  printf("Enter the d prime: ");
  scanf("%d", &dp);

  printf("Final number is: %d",replaceDigit(num, d, dp) );



  

  
}

